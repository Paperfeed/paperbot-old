{
  "name": "paperbot",
  "version": "1.0.0",
  "description": "A Discord bot",
  "scripts": {
    "start": "cross-env NODE_ENV=production ts-node src/Server/index.ts",
    "build": "tsc",
    "dev": "cross-env ts-node-dev --transpile-only --respawn src/Server/index.ts",
    "clean": "rimraf dist",
    "lint": "eslint ./src --ext .ts,.tsx",
    "lint:fix": "eslint --fix  ./src --ext .ts,.tsx",
    "fauna-upload": "fgu",
    "fauna-upload:override": "fgu --override",
    "graphql:generate": "graphql-codegen --require dotenv/config --config codegen.yml"
  },
  "postinstall": "patch-package",
  "dependencies": {
    "@discordjs/opus": "^0.5.0",
    "@graphql-codegen/typescript-graphql-request": "^3.1.1",
    "@types/uuid": "^8.3.0",
    "canvas": "^2.7.0",
    "common-tags": "^1.8.0",
    "concurrently": "^6.0.2",
    "discord.js": "^12.5.3",
    "dotenv": "^8.2.0",
    "fastify": "^3.15.0",
    "fastify-static": "^4.0.1",
    "ffmpeg-static": "^4.3.0",
    "graphql": "^15.5.0",
    "graphql-request": "^3.4.0",
    "igdb-api-node": "^5.0.0",
    "intl-messageformat": "^9.6.11",
    "node-fetch": "^2.6.7",
    "openid": "^2.0.8",
    "patch-package": "^6.4.7",
    "ramda": "^0.27.1",
    "ts-node": "^9.1.1",
    "type-steamapi": "^0.9.46",
    "unique-names-generator": "^4.5.0",
    "uuid": "^3.4.0"
  },
  "devDependencies": {
    "@babel/core": "^7.13.16",
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@graphql-codegen/cli": "1.21.4",
    "@graphql-codegen/introspection": "1.18.2",
    "@graphql-codegen/typescript": "1.22.0",
    "@graphql-codegen/typescript-resolvers": "1.19.1",
    "@igdb/types": "git+https://github.com/twitchtv/igdb-typescript-types.git",
    "@types/common-tags": "^1.8.0",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.11",
    "@types/igdb-api-node": "^5.0.0",
    "@types/intl-messageformat": "^3.0.0",
    "@types/node": "^14.14.41",
    "@types/node-fetch": "^2.5.10",
    "@types/openid": "^2.0.1",
    "@types/ramda": "^0.27.40",
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@typescript-eslint/parser": "^4.22.0",
    "cross-env": "^7.0.3",
    "eslint": "^7.24.0",
    "eslint-config-prettier": "^8.2.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "eslint-plugin-sort-destructure-keys": "^1.3.5",
    "eslint-plugin-sort-keys-fix": "^1.1.1",
    "eslint-plugin-typescript-sort-keys": "^1.6.0",
    "fauna-gql-upload": "^2.1.0",
    "faunadb": "^4.2.0",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "ts-node-dev": "^1.1.6",
    "typescript": "^4.2.4"
  },
  "engines": {
    "node": "14.x"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Paperfeed/paperbot.git"
  },
  "keywords": [
    "discord",
    "nodejs",
    "express"
  ],
  "author": "Aldert Vaandering",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Paperfeed/paperbot/issues"
  },
  "homepage": "https://github.com/Paperfeed/paperbot#readme"
}
